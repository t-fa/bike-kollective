    "@typescript-eslint/parser": "^5.30.0",
    "babel-jest": "^28.1.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "expo-cli": "^5.4.12",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "~4.3.5"
  },
  "private": true
